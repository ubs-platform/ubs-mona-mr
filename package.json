{
    "name": "ubs-mona-mr",
    "version": "5.0.6",
    "description": "",
    "author": "",
    "private": true,
    "license": "MIT",
    "docker": {
        "organisation": "hcangunduz",
        "imageNamePrefix": "ubs-mona-"
    },
    "iksir": {
        "type": "ROOT",
        "childrenVersionTag": "beta",
        "childrenAccess": "public",
        "childrenPrefix": "@ubs-platform"
    },
    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
        "xr": "node ./tools/xr.js",
        "xr:publish-libs": "node ./tools/xr.js publish-libs",
        "xr:patch-libs": "node ./tools/xr.js patch-libs",
        "xr:extend-lib": "node ./tools/xr.js extend-lib",
        "rebuild-xr": "cd ./tools/src && tsc",
        "start": "tools/devserve.sh",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/apps/ubs-mona-mr/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./apps/ubs-mona-mr/test/jest-e2e.json"
    },
    "dependencies": {
        "@nestjs-modules/mailer": "^2.0.2",
        "@nestjs/common": "11.0.7",
        "@nestjs/core": "11.0.7",
        "@nestjs/jwt": "^11.0.0",
        "@nestjs/microservices": "^11.0.7",
        "@nestjs/mongoose": "^11.0.1",
        "@nestjs/passport": "^11.0.5",
        "@nestjs/platform-express": "11.0.7",
        "@nestjs/schedule": "^5.0.1",
        "@nestjs/serve-static": "^5.0.1",
        "axios": "^1.7.9",
        "comment-json": "^4.2.5",
        "crypto-promise": "^2.1.0",
        "kafkajs": "^2.2.4",
        "passport": "^0.7.0",
        "passport-jwt": "^4.0.1",
        "passport-local": "^1.0.0",
        "rxjs": "^7.8.1",
        "sharp": "^0.33.5"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.2.0",
        "@eslint/js": "^9.18.0",
        "@nestjs/cli": "^11.0.0",
        "@nestjs/schematics": "^11.0.0",
        "@nestjs/testing": "^11.0.1",
        "@swc/cli": "^0.6.0",
        "@swc/core": "^1.10.7",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.10.7",
        "@types/supertest": "^6.0.2",
        "eslint": "^9.18.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-prettier": "^5.2.2",
        "globals": "^15.14.0",
        "jest": "^29.7.0",
        "prettier": "^3.4.2",
        "source-map-support": "^0.5.21",
        "supertest": "^7.0.0",
        "ts-jest": "^29.2.5",
        "ts-loader": "^9.5.2",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "types-package-json": "^2.0.39",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.20.0",
        "webpack": "5.97.1"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": ".",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "./coverage",
        "testEnvironment": "node",
        "roots": [
            "<rootDir>/apps/",
            "<rootDir>/libs/"
        ],
        "moduleNameMapper": {
            "^@app/users-roles(|/.*)$": "<rootDir>/libs/users-roles/src/$1",
            "^@mona/users-common(|/.*)$": "<rootDir>/libs/users-common/src/$1",
            "^@mona/users-microservice-helper(|/.*)$": "<rootDir>/libs/users-microservice-helper/src/$1",
            "^@mona/users/core/users-core(|/.*)$": "<rootDir>/libs/users-core/src/$1",
            "^@ubs-platform/crud-base(|/.*)$": "<rootDir>/libs/crud-base/src/$1",
            "^@ubs-platform/crud-base-common(|/.*)$": "<rootDir>/libs/crud-base-common/src/$1",
            "^@ubs-platform/feedback-common(|/.*)$": "<rootDir>/libs/feedback-common/src/$1",
            "^@ubs-platform/file-helper(|/.*)$": "<rootDir>/libs/file-helper/src/$1",
            "^@ubs-platform/microservice-setup-util(|/.*)$": "<rootDir>/libs/microservice-setup-util/src/$1",
            "^@ubs-platform/notify-common(|/.*)$": "<rootDir>/libs/notify-common/src/$1",
            "^@ubs-platform/social-common(|/.*)$": "<rootDir>/libs/social-common/src/$1",
            "^@ubs-platform/social-consts(|/.*)$": "<rootDir>/libs/social-consts/src/$1",
            "^@ubs-platform/users-consts/users-consts(|/.*)$": "<rootDir>/libs/users-consts/src/$1",
            "^mona/core(|/.*)$": "<rootDir>/libs/core/src/$1"
        }
    }
}
